        | publicc error id:e1 llavea {:TablaJava sem = new TablaJava(e1);
                                tabla = sem; TablaJava.AMBITO += 1; reportarSem("Error en la iniciacion de clases, falto la palabra class");:}
        STATEMENTS_CLASS llavec {:tabla.eliminarAmbito(TablaJava.AMBITO); TablaJava.AMBITO -= 1;:}
        | publicc classs error llavea {:TablaJava sem = new TablaJava("");
                                tabla = sem; TablaJava.AMBITO += 1; reportarSem("Error en la iniciacion de clases, falto el id");:}
        STATEMENTS_CLASS llavec {:tabla.eliminarAmbito(TablaJava.AMBITO); TablaJava.AMBITO -= 1;:}
        | publicc classs id:e1 error {:TablaJava sem = new TablaJava(e1);
                                tabla = sem; TablaJava.AMBITO += 1; reportarSem("Error en la iniciacion de clases, falto la palabra class");:}
