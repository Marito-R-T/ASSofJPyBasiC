CASES ::= CASES:e1 casee OPERACION_A:e2 dosp {:cas++;:} STATEMENTS_METODO:e3
        {:cas--; if(e2!=null){RESULT  = e1.agregarCase(e2, e3);}:}
        | {:RESULT = new CasePrograma();:}
        | CASES:e1 casee error dosp STATEMENT_METODO {: reportarSem("Error en case del switch, se esperaba una operacion despues del case");
        RESULT = e1;:}
