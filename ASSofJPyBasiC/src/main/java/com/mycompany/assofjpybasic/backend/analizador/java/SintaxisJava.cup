BLOQUE ::= llavea {:TablaJava.AMBITO += 1;:}  STATEMENTS_METODO:e1 llavec {:tabla.eliminarAmbito(TablaJava.AMBITO); TablaJava.AMBITO -= 1;
                                                RESULT = e1;:}
